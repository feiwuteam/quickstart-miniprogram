  "license": "MIT",
  "scripts": {
    "create": "create-wxapp-page --dir src --style scss --json --tab 2 --name",
    "dev": "cross-env LINT=false NODE_ENV=development webpack --watch --config build/webpack.config.babel.js",
    "dev:prod": "cross-env LINT=false CONF=prod NODE_ENV=development webpack --watch --config build/webpack.config.babel.js",
    "build": "run-s clean && cross-env LINT=false CONF=prod NODE_ENV=production webpack --env.min --config build/webpack.config.babel.js",
    "build:dev": "run-s clean && cross-env LINT=false NODE_ENV=production webpack --env.min --config build/webpack.config.babel.js",
    "lint": "run-s lint:*",
    "lint:css": "stylelint './src/**/*.scss' --fix",
    "lint:ts": "tslint './src/**/*.ts' --fix",
    "clean": "rm -rf dist/ npm-debug.log"
  },
  "copyWebpack": [
    "assets"
  ],
  "dependencies": {
    "@types/es6-shim": "^0.31.37",
    "autoprefixer-loader": "^3.2.0",
